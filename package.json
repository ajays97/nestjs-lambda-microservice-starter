    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/swagger": "^4.7.15",
    "@nestjs/terminus": "^7.1.0",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjsx/crud": "^5.0.0-alpha.3",
    "@nestjsx/crud-typeorm": "^5.0.0-alpha.3",
    "@types/mongodb": "^3.6.8",
