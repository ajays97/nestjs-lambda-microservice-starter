    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "@types/passport-local": "^1.0.33",
    "@types/speakeasy": "^2.0.5",
    "@types/supertest": "^2.0.5",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "serverless-offline": "^6.8.0",
    "serverless-plugin-optimize": "^4.0.2-rc.1",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
